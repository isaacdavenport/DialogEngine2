<!-- Log4net configuration -->
<log4net>
  <appender name="DefaultFileAppender" type="log4net.Appender.RollingFileAppender">
    <param name="File" value="Log/DialogGenerator.log" />
    <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    <appendToFile value="true" />
    <rollingStyle value="Size" />
    <maxSizeRollBackups value="2" />
    <maximumFileSize value="10MB" />
    <staticLogFileName value="true" />
    <layout type="log4net.Layout.PatternLayout">
      <param name="ConversionPattern" value="# %d [%t] %-5p %c %m%n" />
    </layout>
  </appender>

  <appender name="LogDialogFileAppender" type="log4net.Appender.RollingFileAppender">
    <param name="File" value="Log/LogDialog.log" />
    <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    <appendToFile value="true" />
    <rollingStyle value="Size" />
    <maxSizeRollBackups value="2" />
    <maximumFileSize value="10MB" />
    <staticLogFileName value="true" />
    <layout type="log4net.Layout.PatternLayout">
      <param name="ConversionPattern" value="# %d [%t] %-5p %c %m%n" />
    </layout>
  </appender>

  <appender name="DecimalSerialLogFileAppender" type="log4net.Appender.RollingFileAppender">
    <param name="File" value="Log/DecimalSerialLog.log" />
    <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    <appendToFile value="true" />
    <rollingStyle value="Size" />
    <maxSizeRollBackups value="2" />
    <maximumFileSize value="20MB" />
    <staticLogFileName value="true" />
    <layout type="log4net.Layout.PatternLayout">
      <param name="ConversionPattern" value="# %d %m%n" />
    </layout>
  </appender>

    <appender name="DecimalSerialLogDirectBLEFileAppender" type="log4net.Appender.RollingFileAppender">
    <param name="File" value="Log/DecimalSerialLogDirectBLE.log" />
    <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    <appendToFile value="true" />
    <rollingStyle value="Size" />
    <maxSizeRollBackups value="2" />
    <maximumFileSize value="20MB" />
    <staticLogFileName value="true" />
    <layout type="log4net.Layout.PatternLayout">
      <param name="ConversionPattern" value="# %d %m%n" />
    </layout>
  </appender>

  <root>
    <level value="ALL" />
    <appender-ref ref="DefaultFileAppender" />
  </root>
  <logger name="test">
    <level value="DEBUG"></level>
  </logger>

  <logger name="LogDialog" additivity="false">
    <level value="ALL"/>
    <appender-ref ref="LogDialogFileAppender"/>
  </logger>

  <logger name="DecimalSerialLog" additivity="false">
    <level value="ALL"/>
    <appender-ref ref="DecimalSerialLogFileAppender"/>
  </logger>
  
  <logger name="DecimalSerialLogDirectBLE" additivity="false">
    <level value="ALL"/>
    <appender-ref ref="DecimalSerialLogDirectBLEFileAppender"/>
  </logger>

</log4net>
